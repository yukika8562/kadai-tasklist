<div>
  <h1>Login</h1>
</div>

<div>
  <%= form_with(url: login_path,scope: :session, local: true) do |f| %>
  
    <div>
      <%= f.label :userid, 'UserID' %>
      <%= f.text_field :userid %>
    </div>
    <div>
      <%= f.label :password, 'パスワード' %>
      <%= f.text_field :password %>
    </div>
    
    <%= f.submit 'ログイン' %>
  <% end %>
  
  <p><%= link_to '新規ユーザー登録', signup_path %></p>
</div>